<% include base/head %>
<% include base/header %>

  <main>
    <% if (thingsData.length) { %>
      <ul class="things">
      <% for (var i = 0; i < thingsData.length; i++) { %>
        <li class="things--item thing" data-id="<%= thingsData[i]._id %>">
          <p class="thing--content thing--<%= thingsData[i].type %>"><%= thingsData[i].thing %></p>
        </li>
      <% } %>
      </ul>
    <% } %>
  </main>

<% include base/footer %>
